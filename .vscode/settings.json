    "luaide.macroList": [
        {
            "author": "CLark",
            "value": "HIHI",
            // "Date" :${time},
        },
    ],
    // 填入相对路径,${workspaceRoot} 或绝对路径忽略的文件夹集合,不会对忽略文件夹中的脚本进行分析
    "luaide.ignoreDir": [],
    // 填入相对路径,${workspaceRoot} 或绝对路径的文件名,不会对忽略文件夹中的脚本进行分析
    "luaide.ignoreFiles": [],
    // 满足模板创建时文件名和moduleName不同,false为文件和moduleName相同,true为不相同会在创建模板时多输入一个moduleName
    "luaide.inputTemplateModuleName": false,
    // 全局变量颜色
    "luaide.golbalFieldColor": "#00ff00",
    // 全局方法颜色
    "luaide.golbalFunColor": "#00FF00",
    "luaide.apiType": "cocos2dx",
    "markdown.preview.markEditorSelection": false,
    "search.usePCRE2": false, //--------此东东一定要关闭，擦
    "search.useIgnoreFiles": true,
    // 若搜索词全为小写，则不区分大小写进行搜索，否则区分大小写进行搜索
    "search.smartCase": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "res--": true,
        // "entry": true,
        "src--": true,
        "runtime": true,
        "frameworks/runtime-src/proj.android_no_anysdk/assets": true,
        "frameworks/runtime-src/proj.android/assets": true,
        "build/": true,
        "CDN/": true,
        // "temp/": true,
        "library/": true,
        "**/*.anim": true,
    },
    "window.zoomLevel": 1,
