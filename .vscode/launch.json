{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        /////////js
        {
            "name": "Creator Debug: Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:7456",
            "sourceMaps": true,
            "userDataDir": "${workspaceRoot}/.vscode/chrome",
            // "diagnosticLogging": true,
            "trace": true,
            "pathMapping": {
                "/preview-scripts/assets": "${workspaceRoot}/temp/quick-scripts/assets",
                "/": "${workspaceRoot}"
            }
        },
        //////////////////////lua-----------------
        {
            "name": "Cocos2-launch",
            "type": "lua",
            "request": "launch",
            "runtimeType": "Cocos2",
            "localRoot": "${workspaceRoot}",
            "commandLine": "-workdir ${workspaceRoot}/ -file src/main.lua",
            "port": 7003,
            "exePath": "${workspaceRoot}/runtime/mac/ccs-desktop.app/Contents/MacOS/ccs-desktop",
            "printType": 1,
            // "post":0,
        },
        {
            "name": "COCOS(remote debugging)",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Cocos2",
            "localRoot": "${workspaceRoot}",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "Unity-ulua",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Unity",
            "localRoot": "${workspaceRoot}",
            "fileExtname": ".lua",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "Unity-slua",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Unity",
            "localRoot": "${workspaceRoot}",
            "fileExtname": ".txt",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "Unity-xlua",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Unity",
            "localRoot": "${workspaceRoot}",
            "fileExtname": ".txt",
            "port": 7003,
            "printType": 1
        },
        {
            "name": "LuaTest",
            "type": "lua",
            "request": "launch",
            "runtimeType": "LuaTest",
            "mainFile": "${fileBasenameNoExtension}",
            "localRoot": "${fileDirname}",
            "curFileExtname": "${fileExtname}",
            "fileExtname": ".lua",
            "port": 7003,
            "printType": 1
        },

        ////////////////////////typescript
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            // "program": "${workspaceRoot}/assets/src/app.ts",
            "program": "${workspaceRoot}/build/src/app.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            // "externalConsole": false,
            // "console": true,
            "console": "internalConsole",
            "sourceMaps": true,
            // "outDir": null
            "outFiles": [
                "${workspaceRoot}/build/src"
            ],
        },
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:7456",
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "assets/*": "${workspaceRoot}/assets/*"
            },
            "sourceMaps": true,
            "userDataDir": "${workspaceRoot}/.vscode/chrome",
            "diagnosticLogging": false,
            "outFiles": [
                "${workspaceRoot}/dist/**/*.js"
            ]
        },
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/index.html"
        },
        {
            "name": "Attach to url with files served from ./out",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "url": "<url of the open browser tab to connect to>",
            "webRoot": "${workspaceFolder}/out"
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": true,
            // "outDir": null,
            // "outFiles":["${workspaceRoot}/build"],
            "outFiles": [
                "build"
            ],
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        },
        {
            "name": "Attach to Process",
            "type": "node",
            "request": "attach",
            "processId": "${command.PickProcess}",
            "port": 5858,
            "sourceMaps": false,
            // "outDir": null
        }
    ]
}