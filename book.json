{
    "titl":"BlackTech",
    "author":"Clark",
    "description":"视频播放器",
    "language":"zh-hans",
    "root": "./",
    "head_title": "统一规范化保存笔记",
    "head_description": "wiki",
    "head_keywords": "gitbook,文档部署",
    "gitbook": "2.6.9",
    "output.name": "BlackTechBook",
    "links":{
        "siderbar":"Clark-xm.xyz"
    },
    "plugins": [
        "theme-api","theme-comscore","theme-default","theme-faq",
        "search-pro",
        "-insert-logo",
        "-highlight","prism","prism-themes",
        "code",
        "copy-code-button",
        "github" ,
        "-lunr", "-search","search-plus",
        "-back-to-top-button","-page-toc-button","simple-page-toc",
        "chapter-fold",
        "-expandable-chapters-small","-page-treeview","-expandable-chapters",
        "advanced-emoji",
        "emphasize",
        "splitter",
        "-sharing", "sharing-plus",
        "tbfed-pagefooter",
        "page-copyright",
        "sectionx",
        "-fontsettings",
        "ancre-navigation",
        "klipse",
        "donate",
        "change_girls",
        "alerts",
        "pageview-count",
       

    ],
    "pluginsConfig": {
        "theme-api": {
            "theme": "dark"
        },
        
       
        "theme-default": {
            "showLevel": true
        },
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size":  1
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "insert-logo": {
          "url": "./assets/website/images/logo.png",
          "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "code": {
            "copyButtons": false
        },
        "copy-code-button":{

        },
        "github" :{
            "url": "https://github.com/Garretming/blog"
        },
        "search-pro":{},
        "back-to-top-button":{},
        "chapter-fold":{},
        "advanced-emoji":{},
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": true,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "douban", "facebook", "google", "hatenaBookmark", 
                "instapaper", "linkedin","twitter", "weibo", 
                "messenger","qq", "qzone","viber","vk","weibo",
                "pocket", "stumbleupon","whatsapp"
            ]
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy clark-xm.xyz 2019",
            "modify_label": "文件修订时间:",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "page-copyright": {
            "description": "modified at",
            "signature": "Clark",
            "wisdom": "Designer, Frontend Developer & overall web enthusiast",
            "format": "YYYY-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; Clark",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "style": "normal",
            "noPowered": false
        },
        "page-treeview": {
            "copyright": "Copyright &#169; Clark",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "donate": {
            "wechat": "./res/images/qr-wechat-255.png",
            "alipay": "./res/images/qr-alipay-255.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "change_girls" : {
            "time" : 10,
            "urls" : [
                "girlUrl1", "girlUrl2",""
            ]
        },
        "flexible-alerts": {
            "style": "callout",
            "comment": {
              "label": "Comment",
              "icon": "fa fa-comments",
              "className": "info"
            }
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        },
        "edit-link": {
            "base": "https://github.com/Garretming/BlackTech/edit/master",
            "label": "Edit This Page"
        },
        "sectionx": {
            "tag": "b"
        },
        "include-codeblock": {
            "template":"ace",
            "unindent": true,
            "theme": "monokai",
            "edit": true
            
        },
     
        

      





  


  

    }
    
        
}
